# OpenAI API Configuration
REACT_APP_OPENAI_API_KEY=your_credential_openai
REACT_APP_OPENAI_API_URL=https://api.openai.com/v1

# Video Download APIs
REACT_APP_YOUTUBE_DL_API=https://api.youtube-dl.org/api/v1
# RAPIDAPI removido - não será mais usado

# Alternative Video Download Service
REACT_APP_COBALT_API_URL=https://api.cobalt.tools

# Application Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_MAX_FILE_SIZE=100000000
REACT_APP_SUPPORTED_FORMATS=mp4,mp3,wav,m4a

# Authentication (Local Storage)
REACT_APP_JWT_SECRET=your_jwt_secret_for_local_auth
REACT_APP_SESSION_TIMEOUT=3600000

# Easypanel Deployment Variables
REACT_APP_ENVIRONMENT=production
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error