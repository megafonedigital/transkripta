# OpenAI API Configuration
REACT_APP_OPENAI_API_KEY=sk-proj-YOUR_ACTUAL_OPENAI_API_KEY_HERE
REACT_APP_OPENAI_API_URL=https://api.openai.com/v1

# Webhook Configuration for Video Processing
REACT_APP_WEBHOOK_URL=your_webhook_url_here
REACT_APP_WEBHOOK_SECRET=your_webhook_secret_key
REACT_APP_WEBHOOK_TIMEOUT=60000

# Application Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_MAX_FILE_SIZE=100000000
REACT_APP_SUPPORTED_FORMATS=mp4,mp3,wav,m4a

# Authentication (Local Storage)
REACT_APP_JWT_SECRET=your_jwt_secret_for_local_auth
REACT_APP_SESSION_TIMEOUT=3600000

# Admin Credentials - CONFIGURE ANTES DE USAR EM PRODUÇÃO!
REACT_APP_ADMIN_USERNAME=
REACT_APP_ADMIN_EMAIL=
REACT_APP_ADMIN_PASSWORD=

# Easypanel Deployment Variables
REACT_APP_ENVIRONMENT=production
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error